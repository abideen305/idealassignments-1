    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css"
    />
    <script>
      addEventListener(
        "load",
        function () {
          setTimeout(hideURLbar, 0);
        },
        false
      );

      function hideURLbar() {
        window.scrollTo(0, 1);
      }
    </script>

    <!-- Custom Theme files -->
    <!-- <link href="css/bootstrap.css" type="text/css" rel="stylesheet" media="all"> -->
    <link href="css/submit.css" type="text/css" rel="stylesheet" media="all" />
    <!-- font-awesome icons -->
    <link href="css/font-awesome.min.css" rel="stylesheet" />
    <!-- //Custom Theme files -->
    <!-- online-fonts -->
    <link
      href="//fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i"
      rel="stylesheet"
    />
    <link
      href="//fonts.googleapis.com/css?family=Lato:100,100i,300,300i,400,400i,700,700i,900,900i"
      rel="stylesheet"
    />
  </head>

  <body>
    <header>
      <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <button
          class="navbar-toggler"
          type="button"
          data-toggle="collapse"
          data-target="#navbarTogglerDemo01"
          aria-controls="navbarTogglerDemo01"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarTogglerDemo01">
          <a class="navbar-brand" href="/">
            <img src="images/logo bar.jpeg" alt="Logo" style="width: 100px" />
          </a>
          <a class="navbar-brand" href="/">Ideal Assigments Help</a>
          <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
            <li class="nav-item active">
              <a class="nav-link" href="/"
                >Home <span class="sr-only">(current)</span></a
              >
            </li>
            <li class="nav-item">
              <a class="nav-link active" href="/dashboard">Dashboard</a>
            </li>
            <li class="nav-item">
              <a class="nav-link active" href="/transactions">Transaction</a>
            </li>
          </ul>
          <form class="form-inline my-2 my-lg-0">
            <input
              class="form-control mr-sm-2"
              type="search"
              placeholder="Search"
              aria-label="Search"
            />
          </form>
        </div>
      </nav>
    </header>

    <div class="banner_form">
      <form
        class="submitPanel"
        action="/submit"
        method="POST"
        enctype="multipart/form-data"
      >
        <h2>Submit Assignments</h2>
        <br />
        <br />
        <br />
        <div
          style="
            background-color: rgb(55, 48, 48) !important;
            text-align: center;
            padding: 5px;
          "
        >
          <%- include('./partials/messages.ejs') %>
        </div>
        <input
          type="email"
          class="email"
          name="email"
          value="<%if( isAuthenticated === true ) { %>
        <%=user.email%> <% } %>"
          style="display: <%if( isAuthenticated === true )
        { %> none <% } %>"
          id=""
          placeholder="Email"
        />
        <small style="color: black"
          ><%if(typeof email != 'undefined' ) { %> <%=email %> <% } %></small
        >
        <br />
        <input
          type="text"
          placeholder="Enter Title"
          class="country"
          name="title"
        />
        <small style="color: black"
          ><%if(typeof title != 'undefined' ) { %> <%=title %> <% } %></small
        >
        <br />
        <input
          type="text"
          placeholder="Enter Country"
          class="country"
          name="country"
        />
        <small style="color: black"
          ><%if(typeof country != 'undefined' ) { %> <%=country %> <% }
          %></small
        >
        <br />

        <select class="selectSubject" name="subject">
          <option value="" style="display: none">Select Subject</option>
          <option value="General Assignments">General Assignments</option>
          <option value="Statistical Assignments">
            Statistical Assignments
          </option>
          <option value="Essay Writing">Essay Writing</option>
          <option value="Business Plan">Business Plan</option>
        </select>
        <small style="color: black"
          ><%if(typeof subject != 'undefined') { %> <%= subject %> <% }
          %></small
        ><br />
        <input
          type="text"
          placeholder="Deadline"
          class="some_class deadline-home"
          name="deadline"
        />
        <small style="color: black"
          ><%if(typeof deadline != 'undefined') { %> <%=deadline %> <% }
          %></small
        ><br />
        <input
          type="file"
          name="assignment_file"
          placeholder="Upload Question"
          required
        />
        <br />
        <input type="hidden" id="from" name="from" value="" />

        <button class="right_btn submit-home">Submit Now</button>
      </form>
    </div>

    <!-- Bootstrap core JavaScript -->
    <script type="text/javascript">
      document.getElementById("from").value = window.location.pathname;
    </script>
    <script
      src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
      integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js"
      integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"
      integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
      crossorigin="anonymous"
    ></script>

