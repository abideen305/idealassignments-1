    <div class="tab_main tabs">
      <section class="tab-content">
        <div class="tab active" id="tools">
          <div class="New_assignment" id="wrapper">
            <div class="noordr">
              <h3 class="links-fixed">All Transaction</h3>

              <!-- <img src="/images/no-order.svg"> -->
              <!-- Current Order not found
          <p>Looks like you havenâ€™t added any assignment yet.</p> -->
              <table id="table_assignment" class="" style="width: 90%">
                <thead>
                  <tr>
                    <th>Refernce</th>
                    <th>Amount</th>
                    <th>Status</th>
                    <th>Date Created</th>
                    <th>Action</th>
                  </tr>
                </thead>
                <tbody>
                  <% for(var i=0; i < data.length; i++) { %>
                  <tr>
                    <td><%= data[i].reference %></td>
                    <td><%= data[i].amount %></td>
                    <td><%= data[i].status %></td>
                    <td>
                      <%= new Date(data[i].createdAt ).toISOString().slice(0,
                      10) %>
                    </td>
                    <td>
                      <a href="/verify_transaction/<%= data[i].reference %>"
                        >Requery</a
                      >
                    </td>
                  </tr>
                  <% } %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
        <div id="guides" class="tab MyOders" style="display: none"></div>
      </section>
    </div>

    <script
      src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
